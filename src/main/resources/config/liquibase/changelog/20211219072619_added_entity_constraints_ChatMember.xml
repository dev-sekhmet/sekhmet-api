<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity ChatMember.
    -->
    <changeSet id="20211219072619-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="chat_id"
                                 baseTableName="chat_member"
                                 constraintName="fk_chat_member__chat_id"
                                 referencedColumnNames="id"
                                 referencedTableName="chat"/>

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="chat_member"
                                 constraintName="fk_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="skh_user"/>
    </changeSet>
</databaseChangeLog>
